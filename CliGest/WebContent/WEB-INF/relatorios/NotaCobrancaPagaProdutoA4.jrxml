	<queryString>
		<![CDATA[SELECT
       cfl.FK_criar_fac_convenio,
       cfl.FK_factura,
       fc.numero_factura,
       fc.Data_da_facturacao,
       p.FK_paciente,
       pac.Titular,
       pac.nomeCompleto,
       pac.NumeroProcesso,
       year(from_days((to_days(now()) - to_days(cid.data_nascimento)))) AS 'idade',
       p.comparticipacao_paciente,
       p.comparticipacao_aseguradora,
       p.servico,
       p.qtd,
       fv.totalpago,
       fv.numero_fac_paga_convenio,
       fv.nomecompleto as 'funcionario',
       fv.empresa,
       fv.data_pagamento,
       d.nome_comercial, d.pais, d.provincia, d.bairro, d.rua, d.tel1, d.tel2,
	     d.site, d.email, d.nif
FROM
     tblcriarfacturaconveniocomlinhas cfl JOIN tblfactura fc ON cfl.FK_factura = fc.id_factura
     JOIN tblfacturaplanodesaude fpl ON fc.id_factura = fpl.FK_factura
     JOIN vwpacientesnoplano pac ON fpl.FK_titular = pac.FK_parente
     JOIN vwpagamentotodosservicosplano p ON p.numero_factura = fc.numero_factura
     JOIN vwfacturaseguradorapagas fv ON fv.fk_criar_factura_plano = cfl.FK_criar_fac_convenio
     JOIN vwcidadaopacientenomecompleto cid ON cid.Entidade = p.FK_paciente
     cross join vwdetalhesdainstituicao d
WHERE p.numero_factura = $P{numero_factura}]]>
	</queryString>
